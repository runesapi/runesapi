  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.0",
    "jest": "^26.0.1",
    "jest-cli": "^26.0.1",
    "jest-junit": "^10.0.0",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "ts-jest": "^26.1.0",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "@tsed/common": "^5.60.6",
    "@tsed/core": "^5.60.6",
    "body-parser": "^1.19.0",
    "express": "^4.17.1"
